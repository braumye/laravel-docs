{
    "name": "braumye/laravel-docs",
    "license": "MIT",
    "authors": [
        {
            "name": "braumye",
            "email": "braum.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.17",
        "erusev/parsedown-extra": "^0.8.1",
        "symfony/dom-crawler": "^5.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Braumye\\LaravelDocs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Braumye\\LaravelDocs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Braumye\\LaravelDocs\\LaravelDocsServiceProvider"
            ]
        }
    }
}
